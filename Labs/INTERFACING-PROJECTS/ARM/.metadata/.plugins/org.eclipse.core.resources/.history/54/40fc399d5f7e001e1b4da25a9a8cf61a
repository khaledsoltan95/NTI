#ifndef DMA1_PRV_H__
#define	DMA1_PRV_H_H_



#define				ALL_FREE			0u
#define				ALL_BUSY			254u
#define				CHANNEL1_FLAG		1u
#define				CHANNEL2_FLAG		2u
#define				CHANNEL3_FLAG		3u
#define				CHANNEL4_FLAG		4u
#define				CHANNEL5_FLAG		5u
#define				CHANNEL6_FLAG		6u
#define				CHANNEL7_FLAG		7u

#define 			SELECTED			1u
#define 			NOT_SELECTED		2u

#define 			ENABLE				1u
#define 			DISABLE				2u

#define 			FROM_PERIPHERAL		1u
#define 			FROM_MEMORY			2u

#define 			EIGHT_BITS			0u
#define 			SIXTEEN_BITS		1u
#define 			THIRTYTWO_BITS		2u

#define 			LOW					0u
#define 			MIDIUM				1u
#define 			HIGH				2u
#define 			VERY_HIGH			3u

#endif
