#ifndef RCC_PRV_H
#define RCC_PRV_H

											/* Buses List */
	#define		RCC_AHB			1u
	#define		RCC_APB2		2u
	#define		RCC_APB1		3u

											/* Peripherals List */
	#define		GPIOA			1u
	#define		GPIOB			2u
	#define		GPIOC			3u
	#define		ADC				4u
	#define		I2C1			5u
	#define		I2C2			6u
	#define		CAN				7u
	#define		TIMER1			8u
	#define		TIMER2			9u
	#define		TIMER3			10u
	#define		TIMER4			11u
	#define		USB				12u
	#define		UART1			13u
	#define		UART2			14u
	#define		LED				15u






#endif 
